{
  "name": "airport-capacity-planner",
  "version": "1.0.0",
  "description": "Airport Capacity Planning Tool",
  "scripts": {
    "start:backend": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true && cd backend && PORT=3001 npm run dev",
    "start:frontend": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && cd frontend && PORT=3000 npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "test:backend-db": "cd backend && npx jest tests/db-connectivity.test.js",
    "test:frontend-api": "cd frontend && npx jest tests/api-connectivity.test.js",
    "test:e2e": "node tests/e2e-connectivity.js",
    "test:all": "./test-all-components.sh",
    "build": "tsc",
    "test": "jest",
    "stand-capacity-ia": "ts-node src/stand-capacity-ia/index.ts",
    "stand-capacity-forecast": "ts-node src/stand-capacity-forecast/index.ts",
    "ia:analyze": "ts-node src/stand-capacity-ia/index.ts analyze --flights ./data/sample/historical-flights/historical-flights.json --stands ./data/sample/stands/stands.json --aircraft ./data/sample/aircraft-types/aircraft-types.json --adjacency ./data/sample/adjacency-rules/adjacency-rules.json --settings ./data/sample/operational-settings.json --output ./output/utilization",
    "ia:report": "ts-node src/stand-capacity-ia/index.ts report --input ./output/utilization --output ./output/reports",
    "forecast:run": "ts-node src/stand-capacity-forecast/index.ts forecast --historical ./data/sample/historical-capacity/historical-capacity.json --infrastructure ./data/sample/infrastructure-changes/infrastructure-changes.json --stands ./data/sample/stands/forecast-stands.json --aircraft ./data/sample/aircraft-types/forecast-aircraft-types.json --airlines ./data/sample/airline-growth/airline-growth.json --markets ./data/sample/market-forecasts/market-forecasts.json --parameters ./data/sample/forecast-parameters.json --settings ./data/sample/operational-settings.json --output ./forecasts/default",
    "forecast:analyze-gap": "ts-node src/stand-capacity-forecast/index.ts analyze-gap --forecast ./forecasts/default/capacity_forecast.json --demand ./data/sample/demand-projections/demand-projections.json --output ./analysis",
    "forecast:strategy": "ts-node src/stand-capacity-forecast/index.ts generate-strategy --gap-analysis ./analysis/capacity_gap_analysis.json --output ./strategies"
  },
  "devDependencies": {
    "@types/commander": "^2.12.5",
